<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Android test suite on Android Phone">

    <parameter name="OS" value="Android"/>
    <parameter name="appType" value="Phone"/>
    <parameter name="deviceType" value="RealDevice"/>
    <parameter name="deviceName" value="Nexus"/>
    <parameter name="version" value="5.1.1"/>
    <parameter name="moduleName" value="NYP"/>
    <parameter name="appName" value="Clock.apk"/>

    <test name="Test Clock" preserve-order="true">
        <classes>
            <class name="testcases.TestFeatures"/>
            <!--<class name="testcases.TestClock"/>-->
            <class name="testcases.TestTimer"/>
            <class name="testcases.TestStopWatch"/>
        </classes>
    </test>

</suite>